{
    "name": "aragon-futarchy",
    "version": "0.0.1",
    "description": "An Aragon app to facilitate futarchy. Powered by Gnosis' conditional token framework.",
    "author": "Federico Luzzi <fedeluzzi00@gmail.com>",
    "license": "GPL-3.0",
    "private": true,
    "scripts": {
        "postinstall": "yarn compile && yarn build-app",
        "build-app": "cd app && npm install && cd ..",
        "compile": "buidler compile --force && buidler compile --config buidler.config.5.js",
        "test": "buidler test --network buidlerevm",
        "start": "buidler compile && buidler start",
        "publish:major": "buidler publish major",
        "publish:minor": "buidler publish minor",
        "publish:patch": "buidler publish patch",
        "lint:code": "eslint .",
        "lint:prettier": "prettier -l .",
        "lint:commit-message": "commitlint -e",
        "lint": "yarn lint:code && yarn lint:prettier",
        "app:verify-initialization-parameters": "node ./scripts/initialization-check",
        "apm:versions": "aragon apm versions",
        "rinkeby:install": "dao install 0x8Ca5149349e33B8C9Ab739FEa47b024FE716957C futarchy-ctf.open.aragonpm.eth --environment rinkeby --app-init-args 0x36bede640D19981A82090519bC1626249984c908 0x03Ce050DAEB28021086Bf8e9B7843d6212c05F7B 0xc778417E063141139Fce010982780140Aa0cD5Ab 0x3D00D77ee771405628a4bA4913175EcC095538da 0x02321745bE4a141E78db6C39834396f8df00e2a0 --set-permissions open",
        "rinkeby:upgrade": "dao upgrade 0x8Ca5149349e33B8C9Ab739FEa47b024FE716957C futarchy-ctf.open.aragonpm.eth --environment rinkeby"
    },
    "devDependencies": {
        "@aragon/buidler-aragon": "^0.2.9",
        "@aragon/cli": "^7.1.6",
        "@aragon/contract-test-helpers": "0.0.1",
        "@aragon/os": "^4.4.0",
        "@commitlint/cli": "^11.0.0",
        "@commitlint/config-conventional": "^11.0.0",
        "@gnosis.pm/conditional-tokens-contracts": "^1.0.3",
        "@gnosis.pm/conditional-tokens-market-makers": "^1.8.1",
        "@nomiclabs/buidler": "^1.4.7",
        "@nomiclabs/buidler-etherscan": "^1.3.0",
        "@nomiclabs/buidler-truffle5": "^1.3.0",
        "@nomiclabs/buidler-web3": "^1.3.0",
        "@realitio/realitio-contracts": "^2.2.3",
        "@truffle/hdwallet-provider": "^1.1.1",
        "babel-eslint": "^10.1.0",
        "canonical-weth": "^1.4.0",
        "dotenv": "^8.2.0",
        "eslint": "^7.9.0",
        "eslint-plugin-react": "^7.20.6",
        "eslint-plugin-react-hooks": "^4.1.2",
        "husky": "^4.3.0",
        "luxon": "^1.25.0",
        "prettier": "^2.1.2",
        "web3": "^1.3.0"
    }
}
